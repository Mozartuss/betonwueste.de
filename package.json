  "homepage": "https://mozartuss.github.io",
  "dependencies": {
    "@react-spring/web": "^10.0.3",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitejs/plugin-react-swc": "^4.2.1",
    "apexcharts": "^5.3.6",
    "colord": "^2.9.3",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "leaflet": "^1.9.4",
    "react": "^19.2.0",
    "react-apexcharts": "^1.8.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.2.4",
    "react-leaflet": "^5.0.0",
    "react-responsive": "^10.0.1",
    "react-scroll": "^1.9.3",
    "react-switch-selector": "^2.3.0",
    "typescript": "5.9.3",
    "use-local-storage": "^3.0.0",
    "vite": "^7.2.0",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "start": "yarn prettier:fix && vite",
    "build": "tsc && vite build",
    "test": "vite test",
    "preview": "vite preview",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx,css,sass,scss,less,html,md}\"",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,css,sass,scss,less,html,md}\"",
    "lint": "tsc --noEmit && eslint src/**/*.{ts,tsx}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/react-scroll": "^1.8.10",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitest/coverage-v8": "^4.0.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "prettier": "3.6.2",
    "sass": "^1.93.3",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx,js,json,css,sass,scss,less,html,md}": "yarn prettier:fix",
    "src/**/*.{ts,tsx}": "yarn lint --fix"
  }
}
